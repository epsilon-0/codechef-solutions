#include<iostream>
#include<cstdio>

using namespace std;

int a[100005];

int update(int x, int y)
{
  return 1;
}

long long count(int x, int y)
{
  return 1;
}

int main()
{
  int n, m, t, x, y;
  cin >> n >> m;
  for(int i = 0; i < n; i++)
    {
      scanf("%1d", a+i);
    }
  /*
  for(int i = 0; i < n; i++)
    {
      cout << a[i] << " ";
    }
  cout << endl;
  */
  for(int i = 0; i < m; i++)
    {
      scanf("%d %d %d", &t, &x, &y);
      if(t==1){
	update(x, y);
      }
      else{
	printf("%lld\n", count(x, y));
      }
    }
      
}
